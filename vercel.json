{
    "version": 2,
    "builds": [
      {
        "src": "server/CannyServer.js",
        "use": "@vercel/node"
      },
      {
        "src": "webpack.config.js",
        "use": "@vercel/webpack"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/CannyServer.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/index.html"
      }
    ]
  }
  